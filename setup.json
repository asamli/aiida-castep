            "castep.castep = aiida_castep.parsers.castep:CastepParser"
        ],
        "aiida.data": [
            "castep.uspdata = aiida_castep.data.usp:UspData",
            "castep.otfgdata = aiida_castep.data.otfg:OTFGData"
        ]

    }
}
